{
  "name": "search-music",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://rojasleon.github.io/find-music-by-spotify-demo",
  "dependencies": {
    "lodash": "^4.17.10",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-redux-loading": "^1.0.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build-css": "stylus -c src/styles/ --out src/styles/",
    "watch-css": "npm run build-css && stylus -c -w src/styles/ --out src/styles/",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "npm-run-all": "^4.1.3",
    "stylus": "^0.54.5"
  }
}
